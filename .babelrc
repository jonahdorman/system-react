{
  "presets": ["@salesforce/babel-preset-design-system-react"],
  "env": {
    "amd": {
      "plugins": ["import-noop"]
    },
    "commonjs": {
      "plugins": ["import-noop"]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  },
  "ignore": [
    "preset/**",
  ],
  plugins: [
    "root-import"
  ]
}